{
	"name": "mooglepost/mooglepost-api-php-client",
	"type": "library",
	"description": "PHP client library for MooglePost API",
	"keywords": [
		"php",
		"client",
		"library",
		"mooglepost",
		"api"
	],
	"authors": [
		{
			"name": "MooglePost",
			"email": "support@mooglepost.com"
		}
	],
	"license": "MIT",
	"require": {
		"php": "^7.2.5 || ^8.0",
		"ext-json": "*",
		"guzzlehttp/guzzle": "^7.0"
	},
	"require-dev": {
		"overtrue/phplint": "^2.2",
		"squizlabs/php_codesniffer": "^3.5",
		"phpunit/phpunit": "^8.5"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"MooglePost\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"MooglePost\\Tests\\": "tests/"
		}
	}
}
